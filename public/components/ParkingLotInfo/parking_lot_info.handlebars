{{#if parking}}
<div class="parking-lot-info">
    <h3 class="parking-lot-info__address">{{parking.address}}</h3>
    <div class="parking-lot-info__container">
        <p class="parking-lot-info__numbers">Свободно: <strong>{{parking.free_lots}}</strong>/{{parking.all_lots}}</p>
        <div class="parking-lot-button-container">
            {{#if inSearch}}
            <img class="map_icon" id="search_show-on-map_button_{{parking.id}}" alt="SVG Icon"
                src="../../resources/images/icons8-map-24.png">
            {{#if isFavorite}}
            <img class="heart_icon" id="search_add-parking_button_{{parking.id}}" alt="SVG icon"
                src="../../resources/images/icons8-heart-filled.svg">
            {{else}}
            <img class="heart_icon" id="search_add-parking_button_{{parking.id}}" alt="SVG icon"
                src="../../resources/images/icons8-heart-outline.svg">
            {{/if}}
            {{else}}
            <img class="map_icon" id="favorites-show-on-map_button_{{parking.id}}" alt="SVG Icon"
                src="../../resources/images/icons8-map-24.png">
            {{#if isFavorite}}
            <img class="heart_icon" id="favorites-delete-button_{{parking.id}}" alt="SVG icon"
                src="../../resources/images/icons8-heart-filled.svg">
            {{/if}}
            {{/if}}
        </div>
    </div>
</div>
{{/if}}